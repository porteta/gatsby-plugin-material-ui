{
  "name": "gatsby-plugin-material-ui",
  "version": "0.1.2",
  "description": "Gatsby plugin that adds SSR support for Material UI",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "material-ui",
    "jss",
    "react-jss"
  ],
  "author": "Tyler Porter <porteta@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "material-ui": "^1.0.0-beta.9",
    "react-jss": "^7.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1"
  }
}
